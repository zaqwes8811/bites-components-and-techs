<?xml version="1.0"?>
<project name="CustomTaskExample" default="main" basedir=".">
	<taskdef name="filesorter" classname="FileSorter">
		<classpath>
			<pathelement location="tst.jar"/>
		</classpath>
	</taskdef>
	<target name="main">
		<filesorter file="input.txt" tofile="output.txt" sfile="hohoho"/>
	</target>
</project>
